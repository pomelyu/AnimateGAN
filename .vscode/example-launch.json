{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Train DCGAN",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--model=DCGAN",
                "--dataroot=data/demo/faces",
                "--dataset_mode=latent2image",
                "--gpu_ids=-1",
                "--niter=10",
                "--niter_decay=0",
                "--print_freq=10",
                "--display_freq=10",
                "--save_epoch_freq=1",
                "--epoch=0",
                "--epoch_count=1",
                "--checkpoints_dir=checkpoints/demo",
                "--name=dcgan_lr0002",
            ]
        },
        {
            "name": "Train WGANGP",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--model=WGANGP",
                "--dataroot=data/demo/faces",
                "--dataset_mode=latent2image",
                "--gpu_ids=-1",
                "--niter=10",
                "--niter_decay=0",
                "--print_freq=10",
                "--display_freq=10",
                "--save_epoch_freq=1",
                "--epoch=0",
                "--epoch_count=1",
                "--checkpoints_dir=checkpoints/demo",
                "--name=dcgan_gp_lr0001",
            ]
        },
        {
            "name": "Train XGAN",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--model=XGAN",
                "--dataset_mode=image2image",
                "--dataA=data/demo/faces",
                "--dataB=data/demo/faces",
                "--load_size=64",
                "--crop_size=64",
                "--gpu_ids=-1",
                "--num_threads=0",
                "--niter=10",
                "--niter_decay=0",
                "--print_freq=10",
                "--display_freq=10",
                "--save_epoch_freq=1",
                "--epoch=0",
                "--epoch_count=1",
                "--checkpoints_dir=checkpoints/demo",
                "--name=xgan_lr0001",
                "--lr=0.0001",
                "--lambda_dann=1",
                "--lambda_sem=1",
                "--lambda_rec=10",
            ]            
        },
        {
            "name": "Train ACGAN",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "console": "integratedTerminal",
            "args": [
                "--model=ACGAN",
                "--dataset_mode=class2image",
                "--dataroot=data/demo_label",
                "--load_size=64",
                "--crop_size=64",
                "--gpu_ids=-1",
                "--num_threads=0",
                "--niter=10",
                "--niter_decay=0",
                "--print_freq=10",
                "--display_freq=10",
                "--save_epoch_freq=1",
                "--epoch=0",
                "--epoch_count=1",
                "--checkpoints_dir=checkpoints/demo",
                "--name=acgan_lr0001",
                "--lr=0.0001",
                "--lambda_class=10",
            ]            
        },
        {
            "name": "Evaluate DCGAN",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/evaluate.py",
            "console": "integratedTerminal",
            "args": [
                "--model=WGANGP",
                "--checkpoints_dir=checkpoints/demo",
                "--name=dcgan_gp_lr0001",
                "--epoch=10",
                "--dataroot=data/demo/faces",
                "--dataset_mode=latent2image",
                "--out_dir=out/demo",
                "--method=inception_score",
                "--gpu_ids=-1",
            ]
        },
    ]
}